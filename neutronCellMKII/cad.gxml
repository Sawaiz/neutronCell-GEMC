<gxml>
    <!--Body of the sensor is Aluminium -->
    <volume name="blankCap"             color="848789" material="G4_Al"             position="0*mm 0*mm 1273.27*mm" rotation="0*deg 180*deg 0*deg" sensitivity="neutronCell" identifiers="blank cap"/>
    <volume name="sensorCap"            color="848789" material="G4_Al"             position="0*mm 0*mm 1273.27*mm" rotation="0*deg 180*deg 0*deg" sensitivity="neutronCell" identifiers="sensor cap"/>
    <volume name="tube"                 color="848789" material="G4_Al"             position="0*mm 0*mm 1273.27*mm" rotation="0*deg 180*deg 0*deg" sensitivity="neutronCell" identifiers="tube"/>

    <!--18-8 Stainless steel for the sensor cap, held in with screws and ORing-->
    <volume name="sensorHolder"         color="e0dfdb" material="scintillator"      position="0*mm 0*mm 1273.27*mm" rotation="0*deg 180*deg 0*deg" sensitivity="neutronCell" identifiers="sensor holder"/>
    <volume name="oRing"                color="000000" material="G4_RUBBER_BUTYL"   position="0*mm 0*mm 1273.27*mm" rotation="0*deg 180*deg 0*deg" sensitivity="neutronCell" identifiers="o-ring"/>

    <!--Four screws that hold the sensorHolder to sensorCap-->
    <volume name="4-40SocketCap0.625"   color="e0dfdb" material="scintillator"      position="0*mm 0*mm 1273.27*mm" rotation="0*deg 180*deg 0*deg" sensitivity="neutronCell" identifiers="screw 0"/>
    <volume name="4-40SocketCap1.625"   color="e0dfdb" material="scintillator"      position="0*mm 0*mm 1273.27*mm" rotation="0*deg 180*deg 0*deg" sensitivity="neutronCell" identifiers="screw 1"/>
    <volume name="4-40SocketCap2.625"   color="e0dfdb" material="scintillator"      position="0*mm 0*mm 1273.27*mm" rotation="0*deg 180*deg 0*deg" sensitivity="neutronCell" identifiers="screw 2"/>
    <volume name="4-40SocketCap3.625"   color="e0dfdb" material="scintillator"      position="0*mm 0*mm 1273.27*mm" rotation="0*deg 180*deg 0*deg" sensitivity="neutronCell" identifiers="screw 3"/>

    <!--Glass tube holds the fiber encased in optical grease-->
    <volume name="glassTube"  style="0" color="FFFFFF" material="scintillator"      position="0*mm 0*mm 1273.27*mm" rotation="0*deg 180*deg 0*deg" sensitivity="neutronCell" identifiers="glass tube"/>
    <volume name="fiber"                color="16ce5a" material="scintillator"      position="0*mm 0*mm 1273.27*mm" rotation="0*deg 180*deg 0*deg" sensitivity="neutronCell" identifiers="fiber"/>
</gxml>